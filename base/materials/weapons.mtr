gfx/lcannon/primary
{  
	twosided
	
	{
		if((time * 4) % 4 == 0)
		map gfx/lcannon/primary_1.jpg
		blend add
	}
	{
		if((time * 4) % 4 == 1)
		map gfx/lcannon/primary_2.jpg
		blend add
	}
	{
		if((time * 4) % 4 == 2)
		map gfx/lcannon/primary_3.jpg
		blend add
	}
	{
		if((time * 4) % 4 == 3)
		map gfx/lcannon/primary_4.jpg
		blend add
	}
}

