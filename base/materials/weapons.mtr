gfx/blaster/orange_particle
{  
  cull disable
  {
    map gfx/blaster/orange_particle.tga
	blend GL_SRC_ALPHA, GL_ONE_MINUS_SRC_ALPHA
    alphaGen vertex
    rgbGen vertex
  }
}

